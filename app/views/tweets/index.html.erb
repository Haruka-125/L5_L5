<% @tweets.each do |e| %>
    <%= e.message %>
    <%= link_to '削除', "/users/#{e.id}", data: { turbo_method: :delete } %>
    <% if session[:login_uid] != nil %>
        <%= link_to 'いいね', likes_path(tweet_id: e.id), data: { "turbo_method": :post }  %>
        <%= link_to 'いいね削除', "/likes/#{e.id}", data: { turbo_method: :delete }  %>
    <% else %>
        <% nil %>
    <% end %>
    <br>
<% end %>

<% if session[:login_uid] != nil %>
    <br>
    <%= link_to "ツイートをする" ,'/tweets/new' %>
<% else %>
    <% nil %>
<% end %>
    
<br>
<%= link_to "登録画面", '/users/new' %>
<br>
<%= link_to "ログイン" ,'/top/main' %>
<br>
<%= link_to "ログアウト", '/top/logout' %>
<br>
<%= link_to "ユーザー一覧", '/users/index' %>